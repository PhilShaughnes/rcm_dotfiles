snippet a
	[${1:text}](${2:http://})

snippet ref
	[${1:text}](${2:#header})

snippet img
	![${1:alt}](${2:http://})

snippet * italic
	*${1}*${0}

snippet * bold
	**${1}**${0}

snippet front
	---
	title: ${1:Title}
	author: Phil
	layout: post
	permalink: `strftime("/%Y/%m/%d")`/${2:slug}.html
	---


